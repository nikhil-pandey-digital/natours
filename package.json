{
  "name": "Adventour",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "npx parcel watch ./public/js/index.js --target default --dist-dir ./public/js/bundled --public-url /js/bundled",
    "build:js": "npx parcel build ./public/js/index.js --target default  --dist-dir ./public/js/bundled --public-url /js/bundled --no-source-maps"
  },
  "author": "Nikhil Pandey",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^3.1.11",
    "pug": "^3.0.2",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^12.0.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "parcel": "^2.11.0",
    "prettier": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "context": "browser",
      "includeNodeModules": true
    }
  }
}
